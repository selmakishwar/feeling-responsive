---
layout: null
permalink: /workshops.geojson
---
{
  "type": "FeatureCollection",
  "features": [
  {% for workshop in site.data.amy.workshops %}
    {% if workshop._is_upcoming %}
      {
        "type": "Feature",
        "geometry": {
          "type": "Point",
          "coordinates":[{{workshop.longitude}},{{workshop.latitude}}]
        },
        "properties": {
          "marker-color": "#2b3990",
          "details": "<a href='{{workshop.url}}'>{{workshop.venue}}: {{workshop.humandate}}</a>"
        }
      }{% unless forloop.last %},{% endunless %}
    {% endif %}
  {% endfor %}
  ]
}
